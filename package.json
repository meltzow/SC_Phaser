{
  "name": "spacecraft",
  "version": "0.0.1",
  "description": "Example of using `https://www.colyseus.io/` with `@geckos.io/phaser-on-nodejs`",
  "scripts": {
    "build:client": "parcel build src/client/public/game.html",
    "build:server": "tsc -p src/server/tsconfig.json",
    "build": "concurrently \"npm:build:*\"",
    "clean": "rm -rf ./build ./dist",
    "serve:client": "parcel watch src/client/public/game.html",
    "serve:server": "nodemon src/server/server.ts",
    "serve": "concurrently \"npm:serve:*\"",
    "start": "node build/server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:meltzow/SC_Phaser.git"
  },
  "keywords": [
    "colyseus",
    "phaser",
    "geckos.io"
  ],
  "author": "meltzow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/meltzow/SC_Phaser/issues"
  },
  "homepage": "https://meltzow.github.io/SC_Phaser/",
  "dependencies": {
    "@babel/runtime-corejs2": "^7.11.0",
    "@colyseus/ecs": "^0.4.0-alpha.4",
    "@colyseus/monitor": "^0.14.22",
    "@colyseus/schema": "^1.0.28",
    "@geckos.io/phaser-on-nodejs": "^1.2.5",
    "colyseus": "^0.14.23",
    "concurrently": "^6.2.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "phaser": "^3.55.2",
    "phaser3-rex-plugins": "^1.1.62",
    "typescript": "^4.4.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.1",
    "colyseus.js": "^0.14.13",
    "jest": "^27.2.5",
    "nodemon": "^2.0.12",
    "parcel": "^2.0.0-rc.0",
    "prettier": "^2.4.0",
    "ts-jest": "^27.0.7",
    "ts-node": "^10.2.1"
  }
}
